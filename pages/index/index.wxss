/**index.wxss**/

.index {
    color: #222;
    font-size: 28rpx;
}

.index__dialog {
    overflow: hidden;
}

.index-header {
    margin-bottom: 40rpx;
}

.index-header-nav {
    display: flex;
    height: 80rpx;
    border-top: 1px solid #f0f0f0;
    border-bottom: 1px solid #f0f0f0;
    justify-content: space-around;
    align-items: stretch;
    margin-bottom: 28rpx;
}

.index-header-nav>view {
    width: 112rpx;
    line-height: 80rpx;
    text-align: center;
    position: relative;
}

.index-header-nav__active {
    color: #f33497;
}

.index-header-border__active {
    position: absolute;
    bottom: 0;
    left: -4rpx;
    height: 4rpx;
    width: 120rpx;
    background-color: #f33497;
    border-radius: 1px;
}

/*作品秀  */

.index-header-works {
    height: 260rpx;
    width: 100%;
    display: flex;
    white-space: nowrap;
}

.index-header-works-item {
    width: 210rpx;
    height: 260rpx;
    display: inline-block;
    margin-left: 10rpx;
    position: relative;
}

.index-header-works-item>image {
    width: 210rpx;
    height: 260rpx;
}

.index-header-works-item>text {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    text-align: center;
}

/*门店环境  */

.index-header-store {
    display: flex;
    white-space: nowrap;
}

.index-header-store-item {
    display: inline-block;
}

.index-header-store-item>image {
    width: 450rpx;
    height: 270rpx;
    margin-left: 20rpx;
}

/*荣耀奖项  */

.index-header-prize {
    display: flex;
    white-space: nowrap;
}

.index-header-prize-item {
    display: inline-block;
}

.index-header-prize-item>image {
    width: 250rpx;
    height: 350rpx;
    margin-left: 20rpx;
}

/*用户好评  */

.index-header-praise {
    display: flex;
    white-space: nowrap;
}

.index-header-praise-item {
    display: inline-block;
}

.index-header-praise-item>image {
    width: 250rpx;
    height: 450rpx;
    margin-left: 20rpx;
}

/*热门  */

.index-swiper {
    position: relative;
    margin: 20rpx 0 18rpx 0;
}

.index-swiper-icon {
    position: absolute;
    top: 0;
    left: 40rpx;
    width: 70rpx;
    height: 70rpx;
}

.index-swiper-content {
    margin-left: 128rpx;
    font-size: 26rpx;
    height: 70rpx;
    line-height: 70rpx;
}

.index-swiper-content-term>text:first-of-type {
    padding: 4rpx 12rpx;
    margin-right: 18rpx;
    color: #f33497;
    border: 1px solid #f33497;
    border-radius: 4rpx;
}

.border-seat {
    height: 20rpx;
    background: #f6f6f6;
}

.index-rush {
    height: 570rpx;
    text-align: center;
}

.index-rush>image {
    height: 510rpx;
    width: 710rpx;
    margin-top: 30rpx;
}

/*活动顶部栏目导航  */

.index-column {
    padding: 20rpx 0;
}

.index-column>view:first-of-type {
    font-size: 34rpx;
    font-weight: bold;
}

.index-column>view:first-of-type>view {
    width: 34rpx;
    height: 4rpx;
    background: #222;
    display: inline-block;
    vertical-align: middle;
}

.index-column>view:first-of-type>text {
    padding: 0 20rpx;
    vertical-align: middle;
}

.index-column>view:last-of-type {
    font-size: 20rpx;
    color: #b6b6b6;
    text-transform : uppercase;
}

/*拼团  */

.index-collage {
    height: 1200rpx;
    text-align: center;
}

.index-collage-items {
    width: 708rpx;
    height: 932rpx;
    position: relative;
    display: inline-block;
}

.index-collage-items>image {
    width: 708rpx;
    height: 932rpx;
}

.index-collage-items>view:first-of-type {
    position: absolute;
    bottom: -110rpx;
    left: 40rpx;
    width: 310rpx;
    height: 520rpx;
    background-color: #b332f7;
}

.index-collage-items>view:last-of-type {
    position: absolute;
    bottom: -110rpx;
    right: 40rpx;
    height: 520rpx;
    width: 310rpx;
    background-color: #b332f7;
}

.index-collage-items>view>view {
    font-size: 30rpx;
    line-height: 30rpx;
    color: #e9d3f4;
    margin-bottom: 20rpx;
}

.index-collage-items>view>view.index-collage-items-title {
    width: 280rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: inline-block;
}

.index-collage-items>view>view:first-of-type {
    text-align: center;
    background: #c7cade;
    font-size: 0;
    line-height: 0;
}

.index-collage-items>view>view:first-of-type>image {
    width: 300rpx;
    height: 300rpx;
    margin-top: 5rpx;
}

.index-collage-items>view>view:nth-of-type(2) {
    color: #f8da68;
    font-size: 40rpx;
    line-height: 40rpx;
}

.index-collage-items>view>view:last-of-type {
    color: #222;
    font-size: 36rpx;
    line-height: 58rpx;
    width: 211rpx;
    height: 58rpx;
    background-color: #f8da68;
    margin: 0 auto;
    border-radius:8rpx;
}

.index-bargin {
    height: 1200rpx;
    text-align: center;
    font-size: 0;
}

.index-bargin-icon {
    width: 250rpx;
    height: 70rpx;
    padding: 20rpx 0;
}

.index-bargin-items {
    width: 708rpx;
    height: 1036rpx;
    position: relative;
    display: inline-block;
}

.index-bargin-items>image {
    width: 708rpx;
    height: 1036rpx;
}

.index-bargin-items-date {
    font-size: 28rpx;
    width: 100%;
    text-align: center;
    color: #fff;
    position: absolute;
    top: 250rpx;
    left: 0;
}

.index-bargin-items-info {
    width: 100%;
    height: 560rpx;
    font-size: 24rpx;
    color: #999;
    position: absolute;
    bottom: 50rpx;
    left: 0;
}

.index-bargin-items-info>view {
    background: #fff;
    height: 270rpx;
    width: 645rpx;
    border-radius: 8rpx;
    margin: 0 auto;
    display: flex;
    align-items: center;
}

.index-bargin-items-info>view+view {
    margin-top: 20rpx;
}

.index-bargin-items-info>view>view:first-of-type {
    width: 180rpx;
    height: 180rpx;
    padding-left: 25rpx;
    padding-right: 30rpx;
}

.index-bargin-items-content {
    text-align: left;
    width: calc(100% - 270rpx);
}

.index-bargin-items-content>view:first-of-type {
    font-size: 34rpx;
    line-height: 34rpx;
    padding-bottom: 14rpx;
    color: #222;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.index-bargin-items-content>view:last-of-type {
    padding: 20rpx 0;
    font-size: 24rpx;
    color: #999;
}

.index-bargin-items-content>view:last-of-type>text:first-of-type {
    font-size: 26rpx;
    color: #666;
}

.index-bargin-items-content>view:last-of-type>text:nth-of-type(2) {
    font-size: 40rpx;
    line-height: 40rpx;
    color: #f33497;
    padding-right: 40rpx;
}

.index-bargin-items-content>image {
    width: 150rpx;
    height: 54rpx;
    float: right;
}

.index-experience {
    height: 544rpx;
    text-align: center;
    font-size: 0;
}

.index-experience-icon {
    width: 250rpx;
    height: 70rpx;
    padding: 20rpx 0;
}

.index-experience-items {
    width: 708rpx;
    height: 384rpx;
    background-color: #e745be;
    display: inline-block;
}

.index-experience-items>image {
    width: 708rpx;
    height: 384rpx;
}

.index-welfare {
    height: 824rpx;
    text-align: center;
    font-size: 0;
}

.index-welfare-icon {
    width: 250rpx;
    height: 70rpx;
    padding: 20rpx 0;
}

.index-welfare-items {
    width: 706rpx;
    height: 672rpx;
    display: inline-block;
    position: relative;
}

.index-welfare-items>image {
    width: 706rpx;
    height: 672rpx;
}

.index-welfare-items-content {
    position: absolute;
    bottom: 40rpx;
    left: 50%;
    width: 620rpx;
    height: 200rpx;
    background-color: #fff;
    transform: translateX(-50%);
    display: flex;
    align-items: center;
    padding: 0 24rpx;
    font-size: 32rpx;
}

.index-welfare-items-content>image {
    position: absolute;
    top: 0;
    left: 0;
    width: 82rpx;
    height: 84rpx;
    z-index: 1000;
}

.index-welfare-items-content>view:first-of-type {
    width: 208rpx;
    height: 160rpx;
    position: relative;
    padding-right: 28rpx;
}

.index-welfare-items-content>view:first-of-type>image {
    width: 208rpx;
    height: 160rpx;
}

.index-welfare-items-content>view:first-of-type>text {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    font-size: 60rpx;
    color: #f33497;
}

.index-welfare-items-content>view:last-of-type {
    min-height: 160rpx;
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    flex-direction: column;
    text-align: left;
}

.index-welfare-items-price {
    width: 100%;
    display: flex;
    justify-content: space-between;
}

.index-welfare-items-price>view:first-of-type {
    color: #f33497;
}

.index-welfare-items-price>view:first-of-type>text:nth-of-type(2) {
    font-size: 45rpx;
    padding-right: 14rpx;
}

.index-welfare-items-price>view:first-of-type>text:last-of-type {
    font-size: 26rpx;
    color: #b6b6b6;
    text-decoration: line-through;
}

.index-welfare-items-price>view:last-of-type {
    width: 128rpx;
    height: 56rpx;
    line-height: 56rpx;
    text-align: center;
    background: linear-gradient(#ff76db, #f33497);
    border-radius: 8rpx;
    color: #fff;
}

/*activity  */

.index-activity {
    background: #f6f6f6;
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
}

.index-activity>view {
    width: 330rpx;
    height: 366rpx;
    margin-bottom: 40rpx;
    position: relative;
}

.index-activity>view>view {
    position: absolute;
    top: 50rpx;
    width: 100%;
    font-size: 36rpx;
    text-align: center;
}

.index-activity>view>image {
    width: 330rpx;
    height: 366rpx;
}

/* 弹框 */

.modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    z-index: 9000;
    color: #fff;
}

.modal-dialog {
    width: 540rpx;
    padding: 30rpx;
    min-height: 572rpx;
    position: fixed;
    top: 100rpx;
    left: calc(50% - 300rpx);
    z-index: 9999;
    background: #f8297e;
    border-radius: 10rpx;
}

.modal-dialog-cancal {
    position: absolute;
    top: 24rpx;
    right: 24rpx;
}

.modal-dialog-header {
    color: #fff;
    font-size: 50rpx;
    text-align: center;
    padding-bottom: 30rpx;
}

.modal-dialog-content {
    overflow-y: auto;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}
